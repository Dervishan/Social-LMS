@model IEnumerable<Social_LMS.Models.User>

@{
    ViewData["Title"] = "AddContact";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}
<section id="main-content">
    <section class="wrapper">
        <div class="row">
            <div class="col-lg-12">
                <h3 class="page-header"><i class="fa fa-user-md"></i> Ekle</h3>
                <ol class="breadcrumb">
                    <li><i class="fa fa-home"></i><a asp-action="Index" asp-controller="Home">Anasayfa</a></li>
                    <li><i class="fa fa-user-md"></i>Onay</li>
                </ol>
            </div>
        </div>
        <table class="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Surname)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Photo)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Surname)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Photo)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <div><a asp-action="AddContact" asp-controller="Users">Ekle</a></div>
        <!-- page end-->
    </section>
</section>